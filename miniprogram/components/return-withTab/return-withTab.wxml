<!-- 返回 -->
<view style="position: fixed; top: {{top}}px; left: 16px; z-index: 1; font-size: 24px;color:#3d3d3d;" bindtap="return">←</view>
<!-- 顶部占位 -->
<view style="height: {{top+70}}px; width: 100%; background-color: {{color}};"></view>

<view style="width: 100%; height: 100%; position: fixed;z-index: -1; background-color: {{color}};"></view>
