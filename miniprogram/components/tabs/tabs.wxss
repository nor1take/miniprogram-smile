/* components/tabs/tabs.wxss */
.tabs{
  backdrop-filter: blur(5px);
  position:fixed;
  top:0;
  width: 100%;
  display: flex;
  align-items: end;
  justify-content:space-around;
}
.tabsContent{
  display: inline-block;
  width: fit-content;
  margin-top: 32rpx;
  font-size: 34rpx;
  align-items: center;

  color: #3d3d3d;
}
.active{
  font-weight: bolder;
  border-bottom: 6rpx solid #3d3d3d;
}